<asp:ListBox runat="server" CssClass="input"/>

<asp:TextBox runat="server" CssClass="input"></asp:TextBox>

<asp:SiteMapPath runat="server" PathSeparator="|" >
    <NodeStyle CssClass="texRuta" />
